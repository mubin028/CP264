int delete_mid_list(List* list){


	if(is_empty_list(list)){
		printf("Error(delete_mid_list): empty list\n");
		return -1;
	}

	int cut=list->size/2;
	if (cut % 2 ==0){
		cut = cut -1;
	}


	//if (cut == )


	return cut;
}
