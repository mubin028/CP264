#include "cqueue.h"
#include <stdio.h>
#include <stdlib.h>
#include <assert.h>

cQueue* create_queue(int capacity){
	cQueue *q = (cQueue*) malloc(sizeof(cQueue));

	if (capacity < 1) {
		printf("Error(create_queue): invalid queue capacity, set to 10\n");
		capacity = 10;

	}
	q->capacity = capacity;
	q->array = (Data*) malloc(sizeof(Data) * capacity);
	q->front = -1;
	q->rear = -1;

	return q;
}
